    <!-- Main engine pitch -->
	<animation>
		<type>rotate</type>
		<object-name>EngineNacelleLeft</object-name>
		<property>/surface-positions/left-engine-pitch-norm</property>
		<factor>180</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-1.5</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>EngineNacelleRight</object-name>
		<property>/surface-positions/right-engine-pitch-norm</property>
		<factor>180</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-1.5</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

    <!-- Gear animations -->
    <animation>
        <type>translate</type>
        <object-name>Gear.FrontLeft</object-name>
        <property>/gear/gear[0]/compression-norm</property>
        <factor>0.3048</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
	<animation>
		<type>translate</type>
		<object-name>Gear.FrontLeft</object-name>
		<property>/gear/gear[0]/position-norm</property>
        <interpolation>
            <entry><ind>0.00</ind><dep>3.7</dep></entry>
            <entry><ind>0.50</ind><dep>3.6</dep></entry>
            <entry><ind>1.00</ind><dep>0.0</dep></entry>
