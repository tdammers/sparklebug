	<animation>
		<type>rotate</type>
		<object-name>EngineNacelleLeft</object-name>
		<property>/surface-positions/left-engine-pitch-norm</property>
		<factor>180</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-1.5</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>EngineNacelleRight</object-name>
		<property>/surface-positions/right-engine-pitch-norm</property>
		<factor>180</factor>
		<offset-deg>0</offset-deg>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-1.5</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

    <animation>
        <type>translate</type>
        <object-name>Gear0</object-name>
        <property>/gear/gear[0]/compression-norm</property>
        <factor>1</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
	<animation>
		<type>rotate</type>
		<object-name>Gear0</object-name>
		<property>/gear/gear[0]/position-norm</property>
		<factor>-70</factor>
        <offset-deg>70</offset-deg>
		<center>
			<x-m>-1</x-m>
			<y-m>0</y-m>
			<z-m>-11</z-m>
		</center>
		<axis>
